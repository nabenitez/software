<p id="notice"><%= notice %></p>

<h1>Search Tutorials</h1>

<table>
  <thead>
    <tr>
      <th>Duration</th>
      <th>Course</th>
      <th>Start</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @search_tutorials.each do |search_tutorial| %>
      <tr>
        <td><%= search_tutorial.duration %></td>
        <td><%= search_tutorial.course %></td>
        <td><%= search_tutorial.start %></td>
        <td><%= link_to 'Show', search_tutorial %></td>
        <td><%= link_to 'Edit', edit_search_tutorial_path(search_tutorial) %></td>
        <td><%= link_to 'Destroy', search_tutorial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Search Tutorial', new_search_tutorial_path %>
